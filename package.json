{
  "name": "ion-fiddles",
  "version": "1.0.0",
  "description": "ION FIDDLES",
  "main": "index.js",
  "config": {
    "hostname": "localhost",
    "port": "3007",
    "apiPort": "3008",
    "fiddle": "error-handling"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run browser-sync\" \"npm run api\"",
    "server": "nodemon ./server.js",
    "browser-sync": "browser-sync start --proxy \"%npm_package_config_hostname%:%npm_package_config_port%\" --files .",
    "api": "nodemon ./api/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcsolanadal/ion-fiddles.git"
  },
