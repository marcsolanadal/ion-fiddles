    "url": "https://github.com/marcsolanadal/ion-fiddles/issues"
  },
  "homepage": "https://github.com/marcsolanadal/ion-fiddles#readme",
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "redis": "^2.7.1",
    "uuid": "^3.0.1"
  }
